{
    "name": "netsuite-ts-snippets",
    "displayName": "Netsuite - TS Snippets Reload",
    "publisher": "AlexandreJacksonCorrea",
    "description": "This extension contains code snippets for Netsuite SuiteApps developed with TypeScript",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.62.0"
    },
    "repository": {
        "url": "https://github.com/AlexandreD3v/ns-ts-snippets_reload",
        "type": "git"
    },
    "icon": "icon.png",
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "typescript",
                "path": "./snippets/snippets.code-snippets"
            }
        ]
    },
    "dependencies": {
        "vsce": "^2.2.0"
    }
}
